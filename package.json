{
  "name": "xander",
  "version": "0.1.0",
  "private": false,
  "module": "main.ts",
  "scripts": {
    "start": "bun server.ts",
    "start:safe": "docker compose up --build",
    "dev": "bun dev:css && bun dev:server",
    "dev:watch":"bun dev",
    "dev:server":"bun --watch server.ts",
    "dev:css":"bunx --watch tailwindcss -i ./src/styles/globals.css -o ./public/global.css",
    "dev:safe": "docker compose -f docker-compose-dev.yml up --build",
    "format": "prettier --write \"**/*.(js|ts)\"  \"**/**/*.(js|ts)\"",
    "db:start": "bun supabase start",
    "db:stop": "bun supabase stop"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/html": "^1.1.1",
    "@elysiajs/jwt": "^1.1.1",
    "@elysiajs/opentelemetry": "^1.1.6",
    "@elysiajs/static": "^1.1.1",
    "@elysiajs/swagger": "^1.1.5",
    "@kitajs/ts-html-plugin": "^4.1.0",
    "@langchain/anthropic": "^0.3.3",
    "@langchain/community": "^0.3.3",
    "@langchain/core": "^0.3.4",
    "@langchain/langgraph": "^0.2.9",
    "@material-tailwind/react": "^2.1.10",
    "@radix-ui/react-icons": "^1.3.0",
    "@supabase/supabase-js": "^2.45.4",


    "dotenv": "^16.4.5",
    "elysia": "latest",

    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bun": "^1.1.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "bun-types": "latest",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "supabase": "^1.204.3",
    "tailwindcss": "^3.4.13"
  }
}
